"UnlitGeneric"
{
	"$basetexture" "vgui\hud\ui_progressbar_horizontal_motion"
//	"$color2" "[.2 2 .1]"
	"$vertexcolor" 1
	"$vertexalpha" 1
	"$no_fullbright" 1
	"$ignorez" 1
	"$additive" "1"

	"$regenscrollspeed" "0"

	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" "$regenscrollspeed"
			"texturescrollangle" 180
		}
		"RegenRate"
		{
			"resultVar" "$regenscrollspeed"
		}
	}

	
}
